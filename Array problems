https://www.geeksforgeeks.org/last-duplicate-element-sorted-array/

const arr = []
let place=0;
if (arr.length <=0 || arr === null){
    return;
}
for(let i=0; i<arr.length;i++){
    if(arr[i]===arr[i+1]){
        place=i+1;
        // console.log(`index: ${index} and i:${i}`)
    }
}

if(place===0){
    console.log("no duplicates")
}else{
    console.log(`index:${index}`)
}
